# Airoha EN7581 platform kernel configuration for current branch
CONFIG_ARCH_AIROHA=y
CONFIG_ARM64=y
CONFIG_ARM64_PAGE_SHIFT=12
CONFIG_ARM64_VA_BITS=48

# CPU Features
CONFIG_NR_CPUS=2
CONFIG_SMP=y
CONFIG_SCHED_MC=y

# Network
CONFIG_NET=y
CONFIG_INET=y
CONFIG_NETDEVICES=y
CONFIG_NET_VENDOR_AIROHA=y
CONFIG_AIROHA_EN8811H_PHY=y

# Ethernet
CONFIG_NET_CORE=y
CONFIG_ETHERNET=y
CONFIG_NET_VENDOR_MEDIATEK=y

# MTD Support
CONFIG_MTD=y
CONFIG_MTD_BLOCK=y
CONFIG_MTD_SPI_NOR=y
CONFIG_MTD_NAND=y
CONFIG_MTD_NAND_ECC_SW_HAMMING=y

# SPI Support
CONFIG_SPI=y
CONFIG_SPI_MASTER=y
CONFIG_SPI_MEM=y

# GPIO
CONFIG_GPIOLIB=y
CONFIG_GPIO_SYSFS=y

# Serial
CONFIG_SERIAL_8250=y
CONFIG_SERIAL_8250_CONSOLE=y
CONFIG_SERIAL_OF_PLATFORM=y

# USB Support
CONFIG_USB=y
CONFIG_USB_SUPPORT=y
CONFIG_USB_XHCI_HCD=y
CONFIG_USB_XHCI_PLATFORM=y
CONFIG_USB_STORAGE=y

# PCI Support
CONFIG_PCI=y
CONFIG_PCIE_AIROHA=y

# Clock
CONFIG_COMMON_CLK=y
CONFIG_COMMON_CLK_EN7523=y

# Pinctrl
CONFIG_PINCTRL=y
CONFIG_PINCTRL_AIROHA=y

# Watchdog
CONFIG_WATCHDOG=y

# RTC
CONFIG_RTC_CLASS=y

# eBPF Support (for DAE proxy)
CONFIG_BPF=y
CONFIG_BPF_SYSCALL=y
CONFIG_BPF_JIT=y
CONFIG_BPF_JIT_ALWAYS_ON=y
CONFIG_CGROUP_BPF=y
CONFIG_XDP_SOCKETS=y
CONFIG_BPF_EVENTS=y

# Networking features for OpenWrt
CONFIG_NETFILTER=y
CONFIG_NETFILTER_ADVANCED=y
CONFIG_BRIDGE=y
CONFIG_VLAN_8021Q=y
CONFIG_NET_SCHED=y
CONFIG_NET_SCH_HTB=y
CONFIG_NET_SCH_FQ_CODEL=y

# File systems
CONFIG_EXT4_FS=y
CONFIG_EXT4_FS_POSIX_ACL=y
CONFIG_EXT4_FS_SECURITY=y
CONFIG_F2FS_FS=y
CONFIG_SQUASHFS=y
CONFIG_SQUASHFS_XZ=y
CONFIG_OVERLAY_FS=y

# Compression
CONFIG_KERNEL_XZ=y
CONFIG_RD_XZ=y
